<script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/prism.min.js"></script>
<script>
function copyToClipboard(index) {
    const codeContent = document.getElementById(`code-content-${index}`).innerText;
    navigator.clipboard.writeText(codeContent).then(() => {
        alert('Code copied to clipboard!');
    }).catch(err => {
        console.error('Failed to copy: ', err);
    });
}

function toggleCode(index) {
    const codeBox = document.getElementById(`code-box-${index}`);
    codeBox.classList.toggle('minimized');
    const button = document.querySelector(`#code-box-${index} .toggle-button`);
    button.textContent = codeBox.classList.contains('minimized') ? 'Show Code' : 'Hide Code';
}

// Initialize all code-boxes to be minimized when the document is ready
document.addEventListener('DOMContentLoaded', () => {
    const codeBoxes = document.querySelectorAll('.code-box');
    codeBoxes.forEach((codeBox, index) => {
        codeBox.classList.add('minimized');
        const button = codeBox.querySelector('.toggle-button');
        if (button) {
            button.textContent = 'Show Code'; // Ensure the button text reflects the minimized state
        }
    });
});


let slideIndex1 = 1;
let slideIndex2 = 1;

showSlides(slideIndex1, 1); // Initialize first slideshow
showSlides(slideIndex2, 2); // Initialize second slideshow

// Function to advance to the next slide
function plusSlides(n, slideshow) {
  if (slideshow === 1) {
    showSlides(slideIndex1 += n, 1);
  } else if (slideshow === 2) {
    showSlides(slideIndex2 += n, 2);
  }
}

// Function to show a specific slide
function showSlides(n, slideshow) {
  let i;
  let slides;

  if (slideshow === 1) {
    slides = document.getElementsByClassName("mySlides1");
    if (n > slides.length) { slideIndex1 = 1 }
    if (n < 1) { slideIndex1 = slides.length }
    for (i = 0; i < slides.length; i++) {
      slides[i].style.display = "none";
      // Pause videos in non-visible slides
      let videos = slides[i].getElementsByTagName("video");
      for (let j = 0; j < videos.length; j++) {
        videos[j].pause();
      }
    }
    slides[slideIndex1 - 1].style.display = "block";
    // Play video in the visible slide
    let visibleVideos = slides[slideIndex1 - 1].getElementsByTagName("video");
    for (let j = 0; j < visibleVideos.length; j++) {
      visibleVideos[j].currentTime = 0;
      visibleVideos[j].play();
    }
  } else if (slideshow === 2) {
    slides = document.getElementsByClassName("mySlides2");
    if (n > slides.length) { slideIndex2 = 1 }
    if (n < 1) { slideIndex2 = slides.length }
    for (i = 0; i < slides.length; i++) {
      slides[i].style.display = "none";
      // Pause videos in non-visible slides
      let videos = slides[i].getElementsByTagName("video");
      for (let j = 0; j < videos.length; j++) {
        videos[j].pause();
      }
    }
    slides[slideIndex2 - 1].style.display = "block";
    // Play video in the visible slide
    let visibleVideos = slides[slideIndex2 - 1].getElementsByTagName("video");
    for (let j = 0; j < visibleVideos.length; j++) {
      visibleVideos[j].currentTime = 0;
      visibleVideos[j].play();
    }
  }
}

// Automatic slide change for both slideshows
function autoSlide() {
  setInterval(function() {
    plusSlides(1, 1);
    plusSlides(1, 2);
  }, 13700); // Change slides every 14 seconds
}

// Start automatic slideshow
autoSlide();

</script>
