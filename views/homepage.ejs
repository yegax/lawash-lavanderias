<script>
function copyToClipboard(index) {
    const codeContent = document.getElementById(`code-content-${index}`).innerText;
    navigator.clipboard.writeText(codeContent).then(() => {
        alert('Code copied to clipboard!');
    }).catch(err => {
        console.error('Failed to copy: ', err);
    });
}

function toggleCode(index) {
    const codeBox = document.getElementById(`code-box-${index}`);
    codeBox.classList.toggle('minimized');
    const button = document.querySelector(`#code-box-${index} .toggle-button`);
    button.textContent = codeBox.classList.contains('minimized') ? 'Show Code' : 'Hide Code';
}

// Initialize all code-boxes to be minimized when the document is ready
document.addEventListener('DOMContentLoaded', () => {
    const codeBoxes = document.querySelectorAll('.code-box');
    codeBoxes.forEach((codeBox, index) => {
        codeBox.classList.add('minimized');
        const button = codeBox.querySelector('.toggle-button');
        if (button) {
            button.textContent = 'Show Code'; // Ensure the button text reflects the minimized state
        }
    });
});


let slideIndex = 1;
showSlides(slideIndex);

// Function to advance to the next slide
function plusSlides(n) {
  showSlides(slideIndex += n);
}

// Function to show a specific slide
function currentSlide(n) {
  showSlides(slideIndex = n);
}

// Function to show the current slide and control video playback
function showSlides(n) {
  let i;
  let slides = document.getElementsByClassName("mySlides");
  if (n > slides.length) { slideIndex = 1 }
  if (n < 1) { slideIndex = slides.length }
  for (i = 0; i < slides.length; i++) {
    slides[i].style.display = "none";
    // Pause all videos in non-visible slides
    let videos = slides[i].getElementsByTagName("video");
    for (let j = 0; j < videos.length; j++) {
      videos[j].pause();
    }
  }
  slides[slideIndex - 1].style.display = "block";
  // Reset and play videos in the visible slide
  let visibleVideos = slides[slideIndex - 1].getElementsByTagName("video");
  for (let j = 0; j < visibleVideos.length; j++) {
    visibleVideos[j].currentTime = 0; // Reset video to the beginning
    visibleVideos[j].play(); // Play the video
  }
}

// Function to automatically change the slide every 14 seconds
function autoSlide() {
  setInterval(function() {
    plusSlides(1);
  }, 13700); // Change slide every 14 seconds
}

// Call the autoSlide function to start automatic slide change
autoSlide();

</script>
